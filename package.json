{
  "name": "wdio-vscode-service",
  "version": "0.0.0",
  "description": "A service to test VSCode extensions from end to end",
  "author": "Christian Bromann <mail@bromann.dev>",
  "homepage": "https://github.com/webdriverio-community/wdio-vscode-service",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "ci": "run-s lint build test",
    "clean": "rimraf ./node_modules yarn-lock.yaml ./dist",
    "build": "tsc -b .",
    "release": "release-it --github.release",
    "release:ci": "yarn release -- --ci --npm.skipChecks --no-git.requireCleanWorkingDir",
    "release:patch": "yarn release -- patch",
    "release:minor": "yarn release -- minor",
    "release:major": "yarn release -- major",
    "test": "run-s test:*",
    "test:lint": "eslint",
    "test:coverage": "jest --coverage",
    "watch": "yarn run build --watch"
  },
  "keywords": [
    "webdriverio",
    "wdio",
    "wdio-service",
    "vscode",
    "tests"
  ],
  "dependencies": {
    "@wdio/logger": "^7.17.3",
    "electron-chromedriver": "^17.0.0",
    "wdio-chromedriver-service": "^7.2.8",
    "webdriverio": "^7.17.4"
  },
  "devDependencies": {
    "@types/chromedriver": "^81.0.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@wdio/cli": "^7.17.4",
    "@wdio/types": "^7.17.3",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-base-typescript": "^1.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-wdio": "^7.17.4",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "release-it": "^14.12.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "files": [
    "dist/*"
  ]
}
